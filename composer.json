{
    "name": "mvieira/oauth2-mailchimp",
    "description": "MailChimp OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "type": "library",
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "mailchimp"
    ],
    "require": {
        "php": ">=5.6",
        "league/oauth2-client": "^1.4"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.1",
        "squizlabs/php_codesniffer": "^2.7",
        "henrikbjorn/phpspec-code-coverage": "^3.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "League\\OAuth2\\Client\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mickael Vieira",
            "email": "contact@mickael-vieira.com",
            "homepage": "https://github.com/mickaelvieira"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
